{
  "name": "salati",
  "private": true,
  "description": "Server for salt application.",
  "version": "1.0.0",
  "homepage": "http://salat.hmounir.com",
  "author": "Hamza Mounir <hamza.pixelle@gmail.com>",
  "main": "server.js",
  "dependencies": {
    "compression": "1.7.4",
    "dns-prefetch-control": "0.1.0",
    "fastify": "3.1.1",
    "fastify-autoload": "0.8.0",
    "fastify-cors": "^2.1.3",
    "fastify-jwt": "^0.10.0",
    "fastify-plugin": "1.5.0",
    "fastify-static": "^2.4.0",
    "fastify-swagger": "2.3.2",
    "frameguard": "3.0.0",
    "hide-powered-by": "1.0.0",
    "hsts": "2.2.0",
    "http-errors": "1.7.2",
    "http-signature": "^1.2.0",
    "http-status-codes": "^1.3.2",
    "ienoopen": "1.1.0",
    "is_js": "0.9.0",
    "jssha": "^2.3.1",
    "jwks-rsa": "^1.4.0",
    "qs": "^6.7.0",
    "x-xss-protection": "1.1.0"
  },
  "devDependencies": {
    "semantic-release": "^15.13.12",
    "prettier": "2.1.1",
    "xo": "^0.24.0"
  },
  "keywords": [
    "fastify"
  ],
  "license": "0BSD",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "semantic-release": "semantic-release",
    "lint": "xo",
    "autofix": "xo --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "xo": {
    "prettier": true,
    "space": true,
